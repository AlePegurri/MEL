 Dictionary initial-mixture                                                                                  
 {                                                                                  
 @Temperature   	1200 K ;                                                                                   
 @Pressure          0.1 atm ;                                                                                  
 @MoleFractions    	P4 1e-05 HCO 0.99999 AR 0  ;                                                                                            
 }                                                                                  
 Dictionary BatchReactor                                                                                  
 {                                                                                  
 @KineticsFolder	mech_tocompile\kinetics ;  
 @Type				Isothermal-ConstantVolume;                                                                                  
 @InitialStatus          initial-mixture;                                                                                  
 @EndTime				1E+300 s;                                                                                  
 @OdeParameters          ode-OpenSMOKE;                                                                                  
 @Volume				1. cm3;                                                                                  
 @Options				Output;                                                                                                                                                                   
 }                                                                                  
 Dictionary ode-OpenSMOKE                                                                                  
 {	                                                                                  
 @OdeSolver OpenSMOKE;                                                                                  
 @AbsoluteTolerance 1e-12;                                                                                  
 @RelativeTolerance 1e-10;                                                                              
// @MinimumStep 		1e-60;                                                                                  
 //@MaximumStep 		1e-3;                                                                                  
 @MaximumNumberOfSteps 100000;                                                                                  
 }                                                                                  
 Dictionary Output                                                                                  
 {                                                                                  
 @OutputFolder	Output ;  
 @OutputSpecies	 P4 R W4_L W3 P1 P2_L P3 HCO ;                                                                                  
 }                                                                                  
